",\n    //     formProps.meta\n    // );\n    return (\n        <div className=\"form__group\">\n            <input\n                {...formProps.input}\n                autoComplete=\"off\"\n                className={`${\n                    formProps.meta.touched\n                        ? formProps.meta.error\n                            ? \"inpuTError\"\n                            : \"inputOK\"\n                        : \"\"\n                }  form__input `}\n                placeholder={formProps.placeholder}\n            />\n            <label htmlFor=\"nom\" className=\"form__label\">\n                {formProps.label}\n            </label>\n            {renderError(formProps.meta)}\n        </div>\n    );\n};\n\nconst renderTextarea = (formProps) => {\n    console.log(\"🚀 ~ file: Contact.jsx ~ line 58 ~ renderTextarea ~ formProps\", formProps.meta)\n    return (\n        <div className=\"form__group\">\n            <textarea\n                name=\"message\"\n                {...formProps.input}\n                autoComplete=\"off\"\n                value={formProps.input.value}\n                onChange={formProps.input.onChange}\n                id=\"message\"\n                cols=\"30\"\n                rows=\"10\"\n                className={`${\n                    formProps.meta.touched\n                        ? formProps.meta.error\n                            ? \"inpuTError\"\n                            : \"inputOK\"\n                        : \"\"\n                }  form__input `}\n                placeholder=\"votre message...\"\n            ></textarea>\n            <label htmlFor=\"massage\" className=\"form__label\">\n                {formProps.label}\n            </label>\n            {renderError(formProps.meta)}\n        </div>\n    );\n};\n\nconst Contact = ({ history, handleSubmit }) => {\n    const [successMess, setSuccessMess] = useState(\n        \"Votre message a été envoyer avec success!  j'y repondrai des que possible ! merci\"\n    );\n\n    const dispatch = useDispatch();\n\n    const newMessage = useSelector((state) => state.messages);\n    const { loading, success } = newMessage;\n\n    // const form = useSelector((state) => state.form);\n\n    useEffect(() => {\n        if (success) {\n            history.push(\"/contact\");\n        }\n    }, [history, success]);\n\n    //_________________________________fonction______________________________________\n    const onHandleSubmit = (formValue) => {\n        console.log(\n            \"🚀 ~ file: Contact.jsx ~ line 75 ~ onHandleSubmit ~ formValue\",\n            formValue\n        );\n        const data = {\n            name: formValue.name,\n            email: formValue.email,\n            message: formValue.message,\n        };\n        dispatch(createMessage(data));\n    };\n    return (\n        <div className=\"contact\">\n            <div className=\"u-center-text u-margin-bottom-big\">\n                <h2 className=\"heading-secondary\">vous pouvez me contacter </h2>\n            </div>\n            <div className=\"spin\">\n                {loading && <Spinner message=\"\" />}\n                {/* {error && <Alert variant=\"danger\">{error}</Alert>} */}\n                {success && <Alert variant=\"success\">{successMess}</Alert>}\n            </div>\n\n            <div className=\"formulaire\">\n                <div className=\"formulaire__form\">\n                    <h4>Laisser moi un message</h4>\n                    <form\n                        onSubmit={handleSubmit(onHandleSubmit)}\n                        className=\"form\"\n                    >\n                        <Field\n                            name=\"name\"\n                            component={renderInput}\n                            label=\"Votre nom !\"\n                            placeholder=\"votre nom...\"\n                        />\n\n                        <Field\n                            name=\"email\"\n                            component={renderInput}\n                            label=\"Votre mail !\"\n                            placeholder=\"votre email...\"\n                        />\n\n                        <Field\n                            name=\"message\"\n                            component={renderTextarea}\n                            label=\"Votre Message !\"\n                        />\n\n                        <button className=\"btn-submit\" type=\"submit\">\n                            <i className=\"fas fa-arrow-right\"></i>\n                            envoyer\n                        </button>\n                    </form>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nconst validate = (formValues) => {\n    const errors = {};\n    if (!formValues.name) {\n        //only ran if user did not enter a name\n        errors.name = \"vous devez entrer un nom !\";\n    }\n\n    if (\n        /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(\n            formValues.email\n        ) !== true\n    ) {\n        errors.email = \"votre adresse email a un problémes !!!\";\n    }\n    if (!formValues.email) {\n        //only ran if user did not enter a name\n        errors.email = \"vous devez entrer votre mail !\";\n    }\n\n    if (!formValues.message) {\n        //only ran if user did not enter a name\n        errors.message = \"vous devez entrer un message !\";\n    }\n\n    return errors;\n};\n\nexport default reduxForm({\n    form: \"messageForm\",\n    validate,\n})(Contact);\n","/media/ni/新加卷/mern-stack/site-perso-dev/site-perso-mern/frontend/src/redux/store.js",[],"/media/ni/新加卷/mern-stack/site-perso-dev/site-perso-mern/frontend/src/redux/reducer/messageReducer.js",[],"/media/ni/新加卷/mern-stack/site-perso-dev/site-perso-mern/frontend/src/redux/type/messageType.js",[],"/media/ni/新加卷/mern-stack/site-perso-dev/site-perso-mern/frontend/src/components/spinner/Spinner.jsx",[],["85","86"],"/media/ni/新加卷/mern-stack/site-perso-dev/s